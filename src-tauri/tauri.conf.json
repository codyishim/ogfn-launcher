{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Launcher",
  "version": "1.0.0",
  "identifier": "Launcher",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Launcher",
        "width": 1200,
        "height": 700,
        "shadow": true,
        "resizable": false,
        "maximizable": false,
        "fullscreen": false,
        "decorations": false,
        "label": "main"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
